{

  "name": "Traceroute",
  "description": "A browser extension that alerts users where the IP address is located on the webpage they are on",

  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://*/"
  ],
  "browser_action": {

      "default_title": "Traceroute",
      "default_icon": "icon.png",

      "default_title": "Set this page's color.",
      "default_icon": "globe.png",

      "default_popup": "popup.html"
  },

  "content_security_policy": 
    "script-src 'self' https://cdnjs.cloudflare.com https://ajax.googleapis.com; object-src 'self'",

  
  "manifest_version": 2
}
